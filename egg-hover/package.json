{
	"name": "egg-hover",
	"displayName": "egg-hover",
	"description": "Hover information tooltips for egg",
	"version": "0.0.2",
	"engines": {
		"vscode": "^1.80.1"
	},
	"categories": [
		"Other"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [{
			"command": "egg-hover.helloWorld",
			"title": "Hello World"
		}]
	},
	"activationEvents": [
		"onLanguage:egg"
	],
	"scripts": {
		"lint": "eslint .",
		"pretest": "pnpm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.80.1",
		"@types/glob": "^8.1.0",
		"@types/mocha": "^10.0.1",
		"@types/node": "20.2.5",
		"eslint": "^8.41.0",
		"glob": "^8.1.0",
		"mocha": "^10.2.0",
		"typescript": "^5.1.3",
		"@vscode/test-electron": "^2.3.2"
	}
}
