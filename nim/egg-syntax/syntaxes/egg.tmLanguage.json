{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Egg",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [
				{
				"name": "keyword.other.unit",
				"begin": ":",
				"end": "\n"
			},
			{
				"name": "keyword.operator",
				"begin": "\\b(str::)\\b",
				"end": " |\n"
			},
			{
				"name": "keyword.operator",
				"begin": "\\b(num::)\\b",
				"end": " |\n"
			},
			{
				"name": "keyword.control.less",
				"match": "\\b(branch|comment|exit|free|goto)\\b"
			},
			{
				"name": "keyword.control.less",
				"match": "\\b(free|branch|exit|goto|dump|echo|read|readnum|numread|sleep|mod|eval|add|+|subtract|sub|-|multiply|mul|\\*|divide|div|\\/|power|pow|\\*\\*|string\\.index|string\\.split)\\b"
			},
			{
				"name": "comment.block",
				"begin": "# ",
				"end": "\n"
			}
		]
		}
	},
	"scopeName": "source.egg"
}