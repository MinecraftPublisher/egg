{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"scopeName": "source.egg",
	"name": "egg",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [
				{
					"name": "keyword.other.unit",
					"begin": "^:",
					"end": "\n"
				},
				{
					"name": "string.regexp",
					"begin": "#>",
					"end": "\n"
				},
				{
					"name": "comment.block",
					"begin": "#",
					"end": "\n"
				},
				{
					"name": "keyword.operator",
					"begin": "str::",
					"end": " |\n"
				},
				{
					"name": "keyword.operator",
					"begin": "num::",
					"end": " |\n"
				},
				{
					"name": "keyword.control.less",
					"match": "^\\b(equals|eq|morethan|more|lessthan|less|morequals|meq|lessequals|leq|and|or|not)\\b"
				},
				{
					"name": "keyword.control.less",
					"match": "^\\b(free|branch|if|exit|goto)\\b"
				},
				{
					"name": "keyword.control.less",
					"match": "^\\b(dump|echo|printf|read|readnum|numread|sleep)\\b"
				},
				{
					"name": "keyword.control.less",
					"match": "^\\b(mod|eval)\\b"
				},
				{
					"name": "keyword.control.less",
					"match": "^\\b(add|subtract|sub|multiply|mul|divide|div|power|pow)\\b"
				},
				{
					"name": "keyword.control.less",
					"match": "^\\b(string\\.index|string\\.split|string\\.append)\\b"
				},
				{
					"name": "keyword.control.less",
					"match": "^\\b(fs\\.read|fs\\.write)\\b"
				},
				{
					"name": "keyword.control.less",
					"match": "^\\b(reg|register)\\b"
				}
			]
		}
	}
}