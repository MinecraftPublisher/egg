{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"scopeName": "source.egg",
	"name": "egg",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [
				{
					"name": "keyword.other.unit",
					"begin": "^:",
					"end": "\n"
				},
				{
					"name": "string.regexp",
					"begin": "#>",
					"end": "\n"
				},
				{
					"name": "comment.block",
					"begin": "#",
					"end": "\n"
				},
				{
					"name": "keyword.operator",
					"begin": "str::",
					"end": " |\n"
				},
				{
					"name": "keyword.operator",
					"begin": "num::",
					"end": " |\n"
				},
				{
					"name": "keyword.control.less",
					"match": "^\\b(free|branch|if|exit|goto|dump|echo|printf|read|readnum|numread|sleep|mod|eval|add|\\+|subtract|sub|\\-|multiply|mul|\\*|divide|div|\\/|power|pow|\\*\\*|string\\.index|string\\.split|fs\\.read|fs\\.write|equals|eq|\\=|morethan|more|\\>|lessthan|less|\\<|morequals|meq|\\>\\=|lessequals|leq|\\<\\=|and|\\&|or|\\||not|\\!|string\\.append|reg|registry)\\b"
				}
			]
		}
	}
}