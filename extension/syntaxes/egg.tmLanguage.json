{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"scopeName": "source.egg",
	"name": "egg",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [
				{
					"name": "keyword.other.unit",
					"begin": "^( |\t)*:",
					"end": " |\n"
				},
				{
					"name": "keyword.operator",
					"begin": "^( |\t)*str::",
					"end": " |\n"
				},
				{
					"name": "keyword.operator",
					"begin": "^( |\t)*num::",
					"end": " |\n"
				},
				{
					"name": "keyword.control.less",
					"match": "^( |\t)*\\b(equals|eq|morethan|more|lessthan|less|morequals|meq|lessequals|leq|and|or|not)\\b"
				},
				{
					"name": "keyword.control.less",
					"match": "^( |\t)*\\b(free|branch|if|exit|goto)\\b"
				},
				{
					"name": "keyword.control.less",
					"match": "^( |\t)*\\b(dump|echo|printf|read|readnum|numread|sleep)\\b"
				},
				{
					"name": "keyword.control.less",
					"match": "^( |\t)*\\b(mod|eval)\\b"
				},
				{
					"name": "keyword.control.less",
					"match": "^( |\t)*\\b(add|subtract|sub|multiply|mul|divide|div|power|pow)\\b"
				},
				{
					"name": "keyword.control.less",
					"match": "^\\b(string\\.index|string\\.split|string\\.append)\\b"
				},
				{
					"name": "keyword.control.less",
					"match": "^( |\t)*\\b(fs\\.read|fs\\.write)\\b"
				},
				{
					"name": "keyword.control.less",
					"match": "^( |\t)*\\b(reg|register)\\b"
				},


				{
					"name": "support.type.property-name",
					"begin": "^( |\t)*#section ",
					"end": "\n"
				},
				{
					"name": "string.regexp",
					"begin": "#>",
					"end": "\n"
				},
				{
					"name": "comment.block",
					"begin": "#",
					"end": "\n"
				}
			]
		}
	}
}